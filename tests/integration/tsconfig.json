{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"strict": true,
		"skipLibCheck": true,
		"types": ["vitest/globals", "node"]
	},
	"include": ["**/*.ts", "**/*.test.ts", "vitest.config.ts"],
	"exclude": ["node_modules", "dist", "results"]
}
