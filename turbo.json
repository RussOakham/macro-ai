{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [".env"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**"],
			"env": [
				"API_KEY",
				"NODE_ENV",
				"APP_ENV",
				"SERVER_PORT",
				"AWS_COGNITO_USER_POOL_ID",
				"AWS_COGNITO_USER_POOL_CLIENT_ID",
				"AWS_COGNITO_USER_POOL_SECRET_KEY",
				"AWS_COGNITO_REGION",
				"AWS_COGNITO_REFRESH_TOKEN_EXPIRY",
				"COOKIE_DOMAIN",
				"COOKIE_ENCRYPTION_KEY",
				"RELATIONAL_DATABASE_URL",
				"RATE_LIMIT_WINDOW_MS",
				"RATE_LIMIT_MAX_REQUESTS",
				"AUTH_RATE_LIMIT_WINDOW_MS",
				"AUTH_RATE_LIMIT_MAX_REQUESTS",
				"API_RATE_LIMIT_WINDOW_MS",
				"API_RATE_LIMIT_MAX_REQUESTS",
				"VITE_API_URL",
				"VITE_API_KEY",
				"AWS_REGION",
				"SERVICE_NAME",
				"SERVICE_VERSION",
				"AWS_EXECUTION_ENV",
				"ENVIRONMENT",
				"CORS_ALLOWED_ORIGIN",
				"CORS_ALLOWED_ORIGINS",
				"TEST_API_ENDPOINT",
				"TEST_API_KEY",
				"OPENAI_API_KEY",
				"SOME_OPTIONAL_CONFIG",
				"GITHUB_ACTIONS",
				"CI",
				"PREVIEW",
				"AWS_ACCESS_KEY_ID",
				"AWS_SECRET_ACCESS_KEY",
				"MODE",
				"VITE_API_BASE_URL",
				"API_BASE_URL",
				"REDIS_URL",
				"DOPPLER_API_KEY",
				"DOPPLER_TOKEN",
				"DOPPLER_TOKEN_STAGING",
				"DOPPLER_TOKEN_DEV",
				"DOPPLER_TOKEN_PROD"
			]
		},
		"build:docker-image": {
			"dependsOn": ["^build:docker-image"],
			"inputs": ["$TURBO_DEFAULT$"],
			"outputs": ["dist/**"]
		},
		"prebuild:docker-image": {
			"cache": false
		},
		"lint": {
			"dependsOn": ["^build"],
			"env": [
				"AWS_REGION",
				"SERVICE_NAME",
				"SERVICE_VERSION",
				"AWS_EXECUTION_ENV",
				"CI",
				"GITHUB_ACTIONS",
				"GITLAB_CI",
				"CIRCLECI",
				"JENKINS_URL",
				"BUILDKITE",
				"PREVIEW",
				"CORS_ALLOWED_ORIGINS",
				"MODE",
				"APP_VERSION",
				"npm_package_version",
				"DATABASE_URL",
				"CUSTOM_DOMAIN_NAME",
				"VITE_API_BASE_URL",
				"API_BASE_URL",
				"REDIS_URL",
				"DOPPLER_API_KEY",
				"DOPPLER_TOKEN"
			]
		},
		"lint:oxlint": {
			"env": [
				"AWS_REGION",
				"SERVICE_NAME",
				"SERVICE_VERSION",
				"AWS_EXECUTION_ENV"
			]
		},
		"lint:eslint": {
			"dependsOn": ["^build", "generate:msw"],
			"env": [
				"AWS_REGION",
				"SERVICE_NAME",
				"SERVICE_VERSION",
				"AWS_EXECUTION_ENV",
				"CI",
				"GITHUB_ACTIONS",
				"GITLAB_CI",
				"CIRCLECI",
				"JENKINS_URL",
				"BUILDKITE",
				"PREVIEW",
				"CORS_ALLOWED_ORIGINS",
				"MODE",
				"APP_VERSION",
				"npm_package_version",
				"DATABASE_URL",
				"CUSTOM_DOMAIN_NAME"
			]
		},
		"lint:fix": {
			"dependsOn": ["^build"],
			"env": [
				"AWS_REGION",
				"SERVICE_NAME",
				"SERVICE_VERSION",
				"AWS_EXECUTION_ENV",
				"CI",
				"GITHUB_ACTIONS",
				"GITLAB_CI",
				"CIRCLECI",
				"JENKINS_URL",
				"BUILDKITE",
				"PREVIEW",
				"CORS_ALLOWED_ORIGINS",
				"MODE",
				"APP_VERSION",
				"npm_package_version",
				"DATABASE_URL",
				"CUSTOM_DOMAIN_NAME"
			]
		},
		"lint:ci": {
			"dependsOn": ["^build"],
			"env": [
				"AWS_REGION",
				"SERVICE_NAME",
				"SERVICE_VERSION",
				"AWS_EXECUTION_ENV",
				"CI",
				"GITHUB_ACTIONS",
				"GITLAB_CI",
				"CIRCLECI",
				"JENKINS_URL",
				"BUILDKITE",
				"PREVIEW",
				"CORS_ALLOWED_ORIGINS",
				"MODE",
				"APP_VERSION",
				"npm_package_version",
				"DATABASE_URL",
				"CUSTOM_DOMAIN_NAME"
			]
		},
		"lint:fix:eslint": {
			"dependsOn": ["^build"],
			"env": [
				"AWS_REGION",
				"SERVICE_NAME",
				"SERVICE_VERSION",
				"AWS_EXECUTION_ENV",
				"CI",
				"GITHUB_ACTIONS",
				"GITLAB_CI",
				"CIRCLECI",
				"JENKINS_URL",
				"BUILDKITE",
				"PREVIEW",
				"CORS_ALLOWED_ORIGINS",
				"MODE",
				"APP_VERSION",
				"npm_package_version",
				"DATABASE_URL",
				"CUSTOM_DOMAIN_NAME"
			]
		},
		"lint:ci:eslint": {
			"dependsOn": ["^build"],
			"env": [
				"AWS_REGION",
				"SERVICE_NAME",
				"SERVICE_VERSION",
				"AWS_EXECUTION_ENV",
				"CI",
				"GITHUB_ACTIONS",
				"GITLAB_CI",
				"CIRCLECI",
				"JENKINS_URL",
				"BUILDKITE",
				"PREVIEW",
				"CORS_ALLOWED_ORIGINS",
				"MODE",
				"APP_VERSION",
				"npm_package_version",
				"DATABASE_URL",
				"CUSTOM_DOMAIN_NAME"
			]
		},
		"lint:md": {
			"inputs": ["**/*.md", ".markdownlint.json"],
			"outputs": []
		},
		"docs:lint": {
			"inputs": ["**/*.md", ".markdownlint.json"],
			"outputs": []
		},
		"type-check": {},
		"test": {
			"cache": false,
			"dependsOn": ["^build"],
			"outputs": [],
			"env": [
				"AWS_REGION",
				"LOG_LEVEL",
				"SERVICE_NAME",
				"SERVICE_VERSION",
				"AWS_EXECUTION_ENV",
				"TEST_VAR",
				"TEST_VAR1",
				"TEST_VAR2",
				"TEST_ENVIRONMENT",
				"TEST_FRONTEND_URL",
				"MODE",
				"ACT_LOCAL"
			]
		},
		"test:ui": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"test:coverage": {
			"cache": false,
			"dependsOn": ["^build"],
			"outputs": ["coverage/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"generate": {
			"cache": false
		},
		"generate:msw": {
			"cache": false
		},
		"clean": {
			"cache": false
		}
	},
	"envMode": "loose"
}
