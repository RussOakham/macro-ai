{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [".env"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**"],
			"env": [
				"API_KEY",
				"NODE_ENV",
				"APP_ENV",
				"SERVER_PORT",
				"AWS_COGNITO_USER_POOL_ID",
				"AWS_COGNITO_USER_POOL_CLIENT_ID",
				"AWS_COGNITO_USER_POOL_SECRET_KEY",
				"AWS_COGNITO_REGION",
				"AWS_COGNITO_ACCESS_KEY",
				"AWS_COGNITO_SECRET_KEY",
				"AWS_COGNITO_REFRESH_TOKEN_EXPIRY",
				"COOKIE_DOMAIN",
				"COOKIE_ENCRYPTION_KEY",
				"NON_RELATIONAL_DATABASE_URL",
				"RELATIONAL_DATABASE_URL",
				"RATE_LIMIT_WINDOW_MS",
				"RATE_LIMIT_MAX_REQUESTS",
				"AUTH_RATE_LIMIT_WINDOW_MS",
				"AUTH_RATE_LIMIT_MAX_REQUESTS",
				"API_RATE_LIMIT_WINDOW_MS",
				"API_RATE_LIMIT_MAX_REQUESTS",
				"REDIS_URL",
				"VITE_API_URL",
				"VITE_API_KEY",
				"AWS_REGION",
				"AWS_LAMBDA_FUNCTION_NAME",
				"AWS_LAMBDA_RUNTIME_API",
				"LAMBDA_RUNTIME_DIR",
				"SERVICE_NAME",
				"SERVICE_VERSION",
				"AWS_LAMBDA_FUNCTION_VERSION",
				"AWS_LAMBDA_LOG_GROUP_NAME",
				"AWS_LAMBDA_LOG_STREAM_NAME",
				"AWS_LAMBDA_FUNCTION_MEMORY_SIZE",
				"AWS_LAMBDA_FUNCTION_TIMEOUT",
				"AWS_EXECUTION_ENV",
				"AWS_LAMBDA_INITIALIZATION_TYPE",
				"ENVIRONMENT",
				"CORS_ALLOWED_ORIGIN",
				"TEST_API_ENDPOINT",
				"TEST_API_KEY",
				"OPENAI_API_KEY",
				"SOME_OPTIONAL_CONFIG",
				"GITHUB_ACTIONS",
				"CI"
			]
		},
		"lint": {
			"env": [
				"AWS_REGION",
				"AWS_LAMBDA_FUNCTION_NAME",
				"AWS_LAMBDA_RUNTIME_API",
				"AWS_LAMBDA_FUNCTION_ARCHITECTURE",
				"LAMBDA_RUNTIME_DIR",
				"SERVICE_NAME",
				"SERVICE_VERSION",
				"AWS_LAMBDA_FUNCTION_VERSION",
				"AWS_LAMBDA_LOG_GROUP_NAME",
				"AWS_LAMBDA_LOG_STREAM_NAME",
				"AWS_LAMBDA_FUNCTION_MEMORY_SIZE",
				"AWS_LAMBDA_FUNCTION_TIMEOUT",
				"AWS_EXECUTION_ENV",
				"AWS_LAMBDA_INITIALIZATION_TYPE",
				"CI",
				"GITHUB_ACTIONS",
				"GITLAB_CI",
				"CIRCLECI",
				"JENKINS_URL",
				"BUILDKITE"
			]
		},
		"lint:md": {
			"inputs": ["**/*.md", ".markdownlint.json"],
			"outputs": []
		},
		"type-check": {},
		"test": {
			"cache": false,
			"dependsOn": ["^build"],
			"env": [
				"AWS_REGION",
				"AWS_LAMBDA_FUNCTION_NAME",
				"AWS_LAMBDA_RUNTIME_API",
				"LAMBDA_RUNTIME_DIR",
				"LOG_LEVEL",
				"SERVICE_NAME",
				"SERVICE_VERSION",
				"AWS_LAMBDA_FUNCTION_VERSION",
				"AWS_LAMBDA_LOG_GROUP_NAME",
				"AWS_LAMBDA_LOG_STREAM_NAME",
				"AWS_LAMBDA_FUNCTION_MEMORY_SIZE",
				"AWS_LAMBDA_FUNCTION_TIMEOUT",
				"AWS_EXECUTION_ENV",
				"AWS_LAMBDA_INITIALIZATION_TYPE",
				"TEST_VAR",
				"TEST_VAR1",
				"TEST_VAR2"
			]
		},
		"test:ui": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"test:coverage": {
			"dependsOn": ["^build"],
			"outputs": ["coverage/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"generate": {
			"cache": false
		},
		"clean": {
			"cache": false
		}
	},
	"envMode": "loose"
}
