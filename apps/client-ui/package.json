{
	"$schema": "http://json.schemastore.org/package",
	"name": "client-ui",
	"license": "MIT",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">=9.0.0"
	},
	"scripts": {
		"dev": "doppler run -- vite",
		"build": "doppler run -- vite build",
		"build:staging": "doppler run -- vite build --mode staging",
		"build:production": "doppler run -- vite build --mode production",
		"format": "prettier --check --cache .",
		"format:fix": "prettier --write --cache .",
		"lint": "doppler run -- pnpm oxlint src && eslint src --ext .ts,.tsx",
		"lint:fix": "doppler run -- pnpm oxlint src --fix && eslint src --ext .ts,.tsx --fix",
		"lint:ci": "doppler run -- pnpm oxlint src --max-warnings=0 && eslint src --ext .ts,.tsx --max-warnings 0",
		"lint:oxlint": "doppler run -- pnpm oxlint src",
		"lint:fix:oxlint": "doppler run -- pnpm oxlint src --fix",
		"lint:ci:oxlint": "doppler run -- pnpm oxlint src --max-warnings=0",
		"lint:eslint": "doppler run -- eslint src --ext .ts,.tsx",
		"lint:fix:eslint": "doppler run -- eslint src --ext .ts,.tsx --fix",
		"lint:ci:eslint": "doppler run -- eslint src --ext .ts,.tsx --max-warnings 0",
		"preview": "doppler run -- vite preview",
		"type-check": "doppler run -- tsc --noEmit --project tsconfig.app.json",
		"test": "doppler run -- vitest run",
		"test:ui": "doppler run -- vitest --ui",
		"test:run": "doppler run -- vitest run",
		"test:coverage": "doppler run -- vitest run --coverage",
		"generate:msw": "doppler run -- npx msw-auto-mock ../../apps/express-api/public/swagger.json -o src/test/mocks/",
		"deploy:amplify": "doppler run -- ./scripts/deploy-amplify.sh",
		"deploy:staging": "doppler run -- AMPLIFY_ENV=staging ./scripts/deploy-amplify.sh",
		"deploy:production": "doppler run -- AMPLIFY_ENV=production ./scripts/deploy-amplify.sh"
	},
	"dependencies": {
		"@ai-sdk/react": "^1.2.12",
		"@hookform/resolvers": "5.2.1",
		"@radix-ui/react-dropdown-menu": "^2.1.15",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-navigation-menu": "^1.2.13",
		"@radix-ui/react-slot": "^1.2.3",
		"@tanstack/react-query": "^5.84.1",
		"@tanstack/react-query-devtools": "^5.84.1",
		"@tanstack/react-router": "^1.130.12",
		"axios": "^1.11.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"input-otp": "^1.4.2",
		"js-cookie": "^3.0.5",
		"lucide-react": "catalog:",
		"pino": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"react-hook-form": "^7.62.0",
		"sonner": "^1.7.4",
		"tailwind-merge": "^2.6.0"
	},
	"devDependencies": {
		"@repo/config-eslint": "workspace:*",
		"@repo/config-testing": "workspace:*",
		"@repo/config-typescript": "workspace:*",
		"@repo/macro-ai-api-client": "workspace:*",
		"@tailwindcss/typography": "^0.5.16",
		"@tanstack/eslint-plugin-query": "^5.83.1",
		"@tanstack/eslint-plugin-router": "^1.130.12",
		"@tanstack/router-devtools": "^1.130.13",
		"@tanstack/router-plugin": "^1.130.15",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@typescript-eslint/parser": "catalog:",
		"@types/js-cookie": "^3.0.6",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@vitejs/plugin-react": "^4.7.0",
		"@vitest/coverage-v8": "catalog:",
		"@vitest/ui": "catalog:",
		"autoprefixer": "^10.4.21",
		"axios-mock-adapter": "^2.1.0",
		"babel-plugin-react-compiler": "catalog:",
		"eslint": "catalog:",
		"eslint-import-resolver-typescript": "catalog:",
		"globals": "catalog:",
		"happy-dom": "^18.0.1",
		"msw-auto-mock": "^0.31.0",
		"postcss": "^8.5.6",
		"tailwindcss": "catalog:",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "catalog:",
		"vite": "^5.4.19",
		"vitest": "catalog:"
	}
}
