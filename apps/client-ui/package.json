{
	"$schema": "http://json.schemastore.org/package",
	"name": "client-ui",
	"license": "MIT",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">=9.0.0"
	},
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"build:staging": "vite build --mode staging",
		"build:production": "vite build --mode production",
		"format": "prettier --check --cache .",
		"format:fix": "prettier --write --cache .",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"lint:ci": "eslint . --max-warnings 0",
		"preview": "vite preview",
		"type-check": "tsc --noEmit --project tsconfig.app.json",
		"test": "vitest run",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"deploy:amplify": "./scripts/deploy-amplify.sh",
		"deploy:staging": "AMPLIFY_ENV=staging ./scripts/deploy-amplify.sh",
		"deploy:production": "AMPLIFY_ENV=production ./scripts/deploy-amplify.sh"
	},
	"dependencies": {
		"@ai-sdk/react": "^1.2.12",
		"@hookform/resolvers": "^3.10.0",
		"@radix-ui/react-dropdown-menu": "^2.1.15",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-navigation-menu": "^1.2.13",
		"@radix-ui/react-slot": "^1.2.3",
		"@tanstack/react-query": "^5.84.1",
		"@tanstack/react-query-devtools": "^5.84.1",
		"@tanstack/react-router": "^1.130.12",
		"axios": "^1.11.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"input-otp": "^1.4.2",
		"js-cookie": "^3.0.5",
		"lucide-react": "catalog:",
		"pino": "catalog:",
		"pino-pretty": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"react-hook-form": "^7.62.0",
		"sonner": "^1.7.4",
		"tailwind-merge": "^2.6.0",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@repo/config-eslint": "workspace:*",
		"@repo/config-typescript": "workspace:*",
		"@repo/macro-ai-api-client": "workspace:*",
		"@tailwindcss/typography": "^0.5.16",
		"@tanstack/eslint-plugin-query": "^5.83.1",
		"@tanstack/eslint-plugin-router": "^1.130.12",
		"@tanstack/router-devtools": "^1.130.13",
		"@tanstack/router-plugin": "^1.130.15",
		"@testing-library/jest-dom": "^6.6.4",
		"@types/js-cookie": "^3.0.6",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@vitejs/plugin-react": "^4.7.0",
		"@vitest/coverage-v8": "catalog:",
		"@vitest/ui": "catalog:",
		"autoprefixer": "^10.4.21",
		"babel-plugin-react-compiler": "catalog:",
		"eslint": "catalog:",
		"eslint-plugin-react": "catalog:",
		"eslint-plugin-react-hooks": "catalog:",
		"globals": "catalog:",
		"jsdom": "^26.1.0",
		"postcss": "^8.5.6",
		"tailwindcss": "catalog:",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "catalog:",
		"vite": "^5.4.19",
		"vitest": "catalog:"
	}
}
