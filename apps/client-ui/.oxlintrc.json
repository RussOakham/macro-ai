{
	"extends": [
		"../../packages/config-oxlint/base.json",
		"../../packages/config-oxlint/node.json",
		"../../packages/config-oxlint/testing.json",
		"../../packages/config-oxlint/react.json"
	],
	"rules": {
		"react/jsx-key": "error",
		"react/jsx-no-duplicate-props": "error",
		"react/jsx-no-undef": "error",
		"react/no-unused-vars": "error",
		"unicorn/filename-case": [
			"error",
			{ "case": "kebabCase", "ignore": ["^\\$"] }
		]
	}
}
