# Production Environment Configuration for AWS Amplify
# This file contains production environment variables for the React frontend
# These values will be used during Amplify build process

# API Configuration - Production
# This will be updated to point to the deployed Lambda API Gateway endpoint
VITE_API_URL=https://api.macro-ai.com/api
VITE_API_KEY=PLACEHOLDER_UPDATE_WITH_PRODUCTION_API_KEY

# Application Configuration
VITE_APP_ENV=production
VITE_APP_NAME=Macro AI
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_DEBUG_LOGGING=false

# Analytics and Monitoring (if needed in future)
# VITE_ANALYTICS_ID=
# VITE_SENTRY_DSN=
