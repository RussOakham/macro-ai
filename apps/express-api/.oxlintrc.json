{
	"extends": ["../../packages/config-oxlint/base.json"],
	"rules": {
		// Express API specific overrides
		"no-console": "off", // Allow console in backend
		"@typescript-eslint/no-explicit-any": "off", // May be needed for express types
		"unicorn/filename-case": [
			"error",
			{ "case": "kebabCase", "ignore": ["^\\$"] }
		]
	},
	"overrides": [
		{
			"files": ["vitest.setup.ts", "**/vitest.setup.ts"],
			"rules": {
				"no-console": "off",
				"eslint/no-undef": "off"
			}
		}
	]
}
