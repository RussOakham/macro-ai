# Docker build environment variables - dummy values for build-time validation
# These are replaced with real values at runtime via Doppler

# API Configuration
API_KEY=dummy-api-key-for-docker-build-only-32-chars-minimum
NODE_ENV=production
APP_ENV=docker-build
SERVER_PORT=3040

# AWS Cognito (dummy values)
AWS_COGNITO_REGION=us-east-1
AWS_COGNITO_USER_POOL_ID=dummy-pool-id
AWS_COGNITO_USER_POOL_CLIENT_ID=dummy-client-id
AWS_COGNITO_REFRESH_TOKEN_EXPIRY=30

# Cookie Settings
COOKIE_DOMAIN=localhost
COOKIE_ENCRYPTION_KEY=dummy-encryption-key-32-chars-minimum

# Database URLs (dummy values)
REDIS_URL=redis://localhost:6379
RELATIONAL_DATABASE_URL=postgresql://user:pass@localhost:5432/db

# OpenAI (dummy value)
OPENAI_API_KEY=sk-dummy-api-key-for-docker-build-only

# Rate Limiting (defaults)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
AUTH_RATE_LIMIT_WINDOW_MS=3600000
AUTH_RATE_LIMIT_MAX_REQUESTS=10
API_RATE_LIMIT_WINDOW_MS=60000
API_RATE_LIMIT_MAX_REQUESTS=60

# CORS (optional)
CORS_ALLOWED_ORIGINS=http://localhost:3000
