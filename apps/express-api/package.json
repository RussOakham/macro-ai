{
	"$schema": "http://json.schemastore.org/package",
	"name": "@repo/express-api",
	"license": "MIT",
	"private": "true",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"build": "tsc && tsx src/utils/swagger/generate-swagger.ts",
		"build:lambda": "pnpm run clean:lambda && pnpm run bundle:lambda",
		"start": "node dist/index.js",
		"dev:tsc": "tsc --watch -preserveWatchOutput",
		"dev:node": "node --enable-source-maps --watch dist/index.js",
		"dev": "tsx src/utils/swagger/generate-swagger.ts && nodemon --config nodemon.json",
		"generate-swagger": "tsx src/utils/swagger/generate-swagger.ts",
		"clean:lambda": "rimraf dist/lambda.bundle.js dist/lambda.bundle.js.map",
		"bundle:lambda": "esbuild src/lambda.ts --bundle --platform=node --target=node20 --format=esm --outfile=dist/lambda.bundle.js --external:aws-sdk --minify --sourcemap",
		"bundle:lambda:dev": "esbuild src/lambda.ts --bundle --platform=node --target=node20 --format=esm --outfile=dist/lambda.bundle.js --external:aws-sdk --sourcemap",
		"package:lambda": "pnpm run build:lambda && cd dist && zip -r ../lambda-deployment.zip lambda.bundle.js lambda.bundle.js.map",
		"db:generate": "drizzle-kit generate",
		"db:push": "drizzle-kit push",
		"lint": "eslint ./src",
		"type-check": "tsc --noEmit",
		"test": "vitest run",
		"test:ui": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:coverage:ui": "vitest --coverage --ui"
	},
	"devDependencies": {
		"@repo/config-eslint": "workspace:*",
		"@repo/config-typescript": "workspace:*",
		"@types/aws-lambda": "^8.10.152",
		"@types/body-parser": "^1.19.6",
		"@types/compression": "^1.8.1",
		"@types/config": "^3.3.5",
		"@types/cookie-parser": "^1.4.9",
		"@types/cors": "^2.8.19",
		"@types/express": "catalog:",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "catalog:",
		"@types/pg": "^8.15.5",
		"@types/supertest": "^6.0.3",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"@vitest/coverage-v8": "catalog:",
		"@vitest/ui": "catalog:",
		"aws-sdk-client-mock": "^4.1.0",
		"drizzle-kit": "^0.31.4",
		"esbuild": "^0.19.12",
		"eslint": "catalog:",
		"eslint-config-prettier": "catalog:",
		"eslint-plugin-drizzle": "^0.2.3",
		"eslint-plugin-import": "catalog:",
		"eslint-plugin-jsx-a11y": "catalog:",
		"eslint-plugin-react": "catalog:",
		"eslint-plugin-react-hooks": "catalog:",
		"eslint-plugin-unused-imports": "catalog:",
		"globals": "catalog:",
		"nodemon": "^3.1.10",
		"prettier": "catalog:",
		"rimraf": "^5.0.10",
		"supertest": "^7.1.4",
		"tsx": "catalog:",
		"typescript": "catalog:",
		"vitest": "catalog:"
	},
	"dependencies": {
		"@ai-sdk/openai": "^1.3.23",
		"@asteasolutions/zod-to-openapi": "^7.3.4",
		"@aws-lambda-powertools/logger": "^2.24.1",
		"@aws-sdk/client-cognito-identity": "^3.859.0",
		"@aws-sdk/client-cognito-identity-provider": "catalog:",
		"@aws-sdk/client-ssm": "catalog:",
		"ai": "^4.3.19",
		"body-parser": "^1.20.3",
		"compression": "^1.8.1",
		"config": "^3.3.12",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^16.6.1",
		"drizzle-orm": "^0.43.1",
		"drizzle-zod": "^0.7.1",
		"express": "^4.21.2",
		"express-rate-limit": "^7.5.1",
		"helmet": "^8.1.0",
		"http-status-codes": "^2.3.0",
		"jsonwebtoken": "^9.0.2",
		"jwk-to-pem": "^2.0.7",
		"node-fetch": "^3.3.2",
		"pg": "^8.16.3",
		"pgvector": "^0.2.1",
		"pino": "catalog:",
		"pino-http": "^10.5.0",
		"pino-pretty": "catalog:",
		"rate-limit-redis": "^4.2.1",
		"redis": "^4.7.1",
		"serverless-http": "^3.2.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"zod": "catalog:",
		"zod-validation-error": "catalog:"
	}
}
