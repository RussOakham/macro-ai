{
	"extends": ["../config-oxlint/base.json"],
	"rules": {
		// TypeScript config specific overrides
		"no-console": "off", // Allow console in TS configs
		"@typescript-eslint/no-var-requires": "off", // TS configs may need require
		"unicorn/filename-case": [
			"error",
			{ "case": "kebabCase", "ignore": ["^\\$"] }
		]
	}
}
