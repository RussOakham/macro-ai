{
  "extends": "./ts-base.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2023"],
    "allowImportingTsExtensions": true,
    "moduleDetection": "force",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "exclude": ["node_modules"]
}
