{
	"$schema": "http://json.schemastore.org/tsconfig",
	"display": "React Client",
	"extends": "./tsconfig-base.json",
	"compilerOptions": {
		/* Base Options: */
		"moduleDetection": "force",
		/* If transpiling with TypeScript: */
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"sourceMap": true,
		/* If your code doesn't run in the DOM: */
		"lib": ["es2022"]
	}
}
