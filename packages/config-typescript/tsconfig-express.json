{
	"$schema": "http://json.schemastore.org/tsconfig",
	"display": "Express Server",
	"extends": "@total-typescript/tsconfig/tsc/no-dom/app",
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["ES2022"],
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"sourceMap": true,
		"outDir": "./dist",
		"allowImportingTsExtensions": true,
		"verbatimModuleSyntax": false
	}
}
